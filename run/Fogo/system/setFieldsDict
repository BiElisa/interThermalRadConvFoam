FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.fluid 0
);

regions
(

	sphereToCell
    {
        centre (297 98 2707);
        radius 180;   
        fieldValues
        (
            volScalarFieldValue alpha.fluid 1
        );
    }

);


